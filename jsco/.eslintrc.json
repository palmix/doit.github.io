{
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "indent": ["error", 2],
        "no-var": "error",
        "comma-dangle": ["error", "never"],
        "new-cap": ["error", {"capIsNewExceptions": ["UglifyJS.Compressor"]}],
        "no-restricted-syntax": 0,
        "import/no-mutable-exports": 0,
        "no-param-reassign": 0,
        "no-await-in-loop": 0
    },
    "env": {
        "browser": true
    }
}
